{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["error", "double"],
    "linebreak-style": "off",
    "no-plusplus": "off",
    "guard-for-in": "off",
    "no-restricted-syntax": "off",
    "prefer-const": "off"
  }
}
